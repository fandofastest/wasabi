{
  "name": "full-static-search",
  "version": "1.3.1",
  "description": "A lib to build an smart search on a static array",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "build:watch": "npm run build -- --watch",
    "test": "mocha --require babel-polyfill --compilers js:babel-register --recursive",
    "lint": "eslint ./src ./test --ext .js",
    "all": "npm run lint && npm test && npm run build",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bieelsoares/full-static-search.git"
  },
  "keywords": [
    "search",
    "array",
    "static",
    "fss",
    "fulltextsearch",
    "fullstaticsearch"
  ],
  "author": "Gabriel Soares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bieelsoares/full-static-search/issues"
  },
  "homepage": "https://github.com/bieelsoares/full-static-search#readme",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.16.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "enum": "^2.4.0"
  }
}
